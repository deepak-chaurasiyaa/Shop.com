                <form action="/signup" method="POST">
                    <div id="form">
                        <div class = "signup">
                            <p class = "heavy ac-text">Create an account</p>
                            <p style="margin-top: -18px;" class = "thin thins">* Indicates required field</p>
                            <div class = "signup-data">
                                
                               <div class="signup-input">
                                <div class="thin">First Name*</div>
                                   <input  id="userFirstName"  class="sign-inp" type="text"  placeholder="First Name here" style="font-style: italic" name="userName" required>
                               </div>
                               <div class="signup-input">
                                <div class="thin" >Last name*</div>
                                <input  id="userLastName" class="sign-inp" type="text" placeholder="Last Name here " style="font-style: italic;">
                               </div>
                               <div class="signup-input">
                                <div class="thin">Email address*</div>
                                <input id="userEmail"  class="sign-inp" type="text" placeholder="yourname@example.com" style="font-style: italic" name="email" required>
                               </div>
                               <div class=""></div>
                               <div class="signup-input">
                                <div class="thin">Password* (At least 7 characters)</div>
                                <input id="userPass"  class="sign-inp" type="password" placeholder="Please enter a minimum of 7 characters" style="font-style: italic"name="password"
                                required>
                               </div>
                               <div class="signup-input">
                                <div class="thin">Confirm password*</div>
                                <input class="sign-inp"  type="password" placeholder="Please re-enter your password" style="font-style: italic;">
                               </div>
                            </div>
                        </div>
                            <div class = "signup-opt">
                                <div class="msg-alrt heavy">Text Message (SMS) Alerts: (Optional)</div>
                                <input class="checkbox" type="checkbox"><span>Do you want to sign up for Shopper alerts from SHOP.COM? (Message and data rates may apply.)</span>
                                <div class="signup-input" style="margin-top: 28px;">
                                    <div class="thin">Nobile Phone</div>
                                    <input class="sign-inp opt-inp" type="text" placeholder="Please enter your 10 digit phone number(i.e. 999 999 9999)" style="font-style: italic"name="mobile" required>
                                </div>
                                <div class = "referal">
                                    <div class="signup-input" style="margin-top: 20px;">
                                    <div style="margin-left: 10px;" class="thin">Were you referred by a friend?
                                    Enter their email address (Optional)</div>
                                    <input style="margin-left: 10px;" class="sign-inp " type="text" placeholder="Please enter your 10 digit phone number(i.e. 999 999 9999)" style="font-style: italic;">
                                </div>
                            </div>
                        </div>
                        <div class = "robot">
                            <p>Your Shop Consultant is SHOP.COM</p>
                            <p class="msg-alrt heavy">Email deals: (Optional)</p>
                            <input class="checkbox" type="checkbox"><span>Do you want to sign up for sweet email offers and discounts from
                                SHOP.COM?</span>
                            <div class="signup-input" style="margin-top: 28px;">
                                <p class="heavy">Also, please confirm you are not a robot:</p> 
                                <div class = "robot-img">
                                    <input id = "check-robot" class="checkbox " type="checkbox"><span>I am not a robot</span>
                                   
                                        <img id = "captcha" class = "captcha" src="data:image/png;base64," alt="">
                                </div>
                            </div> 
                        </div>
                        <button id = "ssignup" class="sign-inp opt-inp opt-inp1 heavy" style="font-style: italic;">Create your new account</button>
                        <p class="opt-inp2">By creating an account, you are agreeing to our Terms of Service and Privacy Policy.</p>
                </form>
